					"slug": "base"
				},
				{
					"color": "rgb(155, 154, 151)",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "rgb(155, 154, 151)",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "rgb(155, 154, 151)",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "rgb(0, 0, 0)",
					"name": "Black",
					"slug": "contrast"
				},
				{
					"color": "rgb(255, 255, 255, 0.4)",
