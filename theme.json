				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"custom": {
			"transition": {
				"normal": "all .2s ease-in-out"
			},
			"backdropFilter": {
				"normal": "blur(25px) brightness(120%)",
				"x-bright": "blur(20px) brightness(250%)"
			},
			"spacing": {
				"blockGap": "1.25em"
			},
			"roundedRadius": {
				"normal": "20px",
				"large": "60px"
			}
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--secondary)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--poppins)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "1.5"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--block-gap)",
			"padding": {
				"top": "0px",
				"right": "var(--wp--preset--spacing--20)",
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--20)"
			}
		},
		"blocks": {
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--block-gap)"
				},
				"typography": {
					"textDecoration": "none"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"border": {
								"bottom": {
									"style": "solid",
									"width": "2px"
								}
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							},
							"border": {
								"bottom": {
									"style": "solid",
									"width": "2px"
								}
							}
						}
					}
				}
			},
			"core/button": {
				"css": "transform: scale(1); &:hover { transform: scale(1.05); } &:focus { transform: scale(1.01); } &:active { transform: scale(0.98); }",
				"variations": {
					"spacing": {
						"padding": {
							"top": "calc(var(--wp--preset--spacing--20) * 0.7)",
							"bottom": "calc(var(--wp--preset--spacing--20) * 0.7)",
							"left": "calc(var(--wp--preset--spacing--20) * 2)",
							"right": "calc(var(--wp--preset--spacing--20) * 2)"
						}
					},
					"outline": {
						"color": {
							"background": "transparent",
							"text": "var(--wp--preset--color--primary)"
						},
						"border": {
							"color": "currentColor",
							"width": "0.15em",
							"radius": "1em"
						}
					}
				}
			}
		},
		"elements": {
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
