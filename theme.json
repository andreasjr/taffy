					"slug": "clear-primary"
				},
				{
					"color": "rgb(0, 0, 0, 0.2)",
					"name": "Clear - Alt",
					"slug": "clear-alt"
				}

			],
			"gradients": [
				{
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--primary-alt), var(--wp--preset--color--base))",
					"name": "Primary to Base",
					"slug": "primary-to-base"
				},
				{
					"gradient": "linear-gradient(180deg, var(--wp--preset--color--secondary-alt), var(--wp--preset--color--base))",
					"name": "Secondary to Base",
					"slug": "secondary-to-base"
				},
				{
					"gradient": "linear-gradient(180deg, rgba(0,0,0,0) 0%, var(--wp--preset--color--clear-alt) 55%, var(--wp--preset--color--contrast) 100%)",
					"name": "Background Fade",
					"slug": "background-fade"
				}
			]
		},
		"typography": {
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"min": "0.5rem",
						"max": "0.75rem"
					},
					"size": "0.75rem",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"fluid": {
						"min": "0.875rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.125rem"
					},
					"size": "1.125rem",
					"slug": "normal",
					"name": "Normal"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "1.875rem"
					},
					"size": "1.75rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "2.25rem"
					},
					"size": "2.25rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2.5rem",
						"max": "3rem"
					},
					"size": "3rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "2.8rem",
						"max": "3.8rem"
					},
					"size": "3.8rem",
					"slug": "xx-large",
					"name": "Extra Extra Large"
				}
			],
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
