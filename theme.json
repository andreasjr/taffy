							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/montserrat/Montserrat-ExtraBold.ttf"
							]
						},
						{
							"fontFamily": "Montserrat",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/montserrat/Montserrat-ExtraBoldItalic.ttf"
							]
						}
					],
					"fontFamily": "\"Montserrat\", sans-serif",
					"name": "Montserrat",
					"slug": "montserrat"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Permanent Marker",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/permanent_marker/PermanentMarker-Regular.ttf"
							]
						}
					],
					"fontFamily": "\"Permanent Marker\", sans-serif",
