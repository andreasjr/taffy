		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(0.2rem, 2vw, 0.5rem)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(0.5rem, 5vw, 1rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "7"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "8"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"custom": {
			"transition": {
				"normal": "all .2s ease-in-out"
			},
			"backdropFilter": {
				"normal": "blur(25px) brightness(120%)",
				"x-bright": "blur(20px) brightness(250%)"
			},
			"spacing": {
				"blockGap": "1.25em"
			},
			"roundedRadius": {
				"normal": "20px",
				"large": "60px"
			}
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"gradient": "var(--wp--preset--gradient--primary-to-base)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "1.33"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--block-gap)",
			"padding": {
                "top": "0px",
                "right": "var(--wp--preset--spacing--20)",
                "bottom": "0px",
                "left": "var(--wp--preset--spacing--20)"
            }
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"fontWeight": "800",
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"letterSpacing": "1px",
					"lineHeight": "1.2"
				}
			},
			"core/post-content": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--block-gap)"
				}
			},
			"core/navigation": {
				"spacing": {
					"blockGap": "var(--wp--custom--spacing--block-gap)"
				},
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/navigation-link": {
				"css": "& li:hover { border-bottom: solid 2px; }"
			},
			"core/page-list": {
				"css": "& li:hover { border-bottom: solid 2px; } & li.current-menu-item { border-bottom: solid 2px; }"
			},
			"core/button": {
				"css": "transform: scale(1); &:hover { transform: scale(1.05); } &:focus { transform: scale(1.01); } &:active { transform: scale(0.98); }"
			}
		},
		"elements": {
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color":{
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--montserrat)",
					"lineHeight": "1.2"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "700"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "400"
				}
			},
			"button": {
				"border": {
					"radius": "1.25em",
					"style": "solid",
					"width": "0.25em"
				},
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textTransform": "uppercase",
					"fontWeight": "700",
					"letterSpacing": "0.0625em"
				},
				"spacing": {
					"padding": {
						"top": "1.125em",
						"bottom": "1.125em",
						"left": "2.1875em",
						"right": "2.1875em"
					}
				},
				"shadow": "var(--wp--preset--shadow--normal)",
				":hover": {
					"shadow": "var(--wp--preset--shadow--large)"
				},
				":focus": {
					"shadow": "var(--wp--preset--shadow--large)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "footer",
			"name": "footer-contact-information",
			"title": "Footer Contact Information"
		}
	],
	"customTemplates": [
		{
			"name": "page-noheader",
			"title": "Page (No Header)"
		}
	],
	"version": 2
}