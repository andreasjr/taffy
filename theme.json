					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "300"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "400"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "400"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontWeight": "400"
				}
			},
			"button": {
				"border": {
					"radius": "1em",
					"style": "solid",
					"width": "0.15em",
					"color": "transparent"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"textTransform": "uppercase",
					"fontWeight": "500",
					"letterSpacing": "0.0625em",
					"fontFamily": "var(--wp--preset--font-family--poppins)"
				},
				"spacing": {
					"padding": {
						"top": "calc(var(--wp--preset--spacing--20) * 0.8)",
						"bottom": "calc(var(--wp--preset--spacing--20) * 0.8)",
						"left": "calc(var(--wp--preset--spacing--20) * 2)",
						"right": "calc(var(--wp--preset--spacing--20) * 2)"
					}
				}
			}
		}
	},
